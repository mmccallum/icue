{
  "name": "corsair-icue",
  "version": "0.1.0",
  "description": "Node.js binding for Corsair iCUE SDK - control RGB lights and detect G key presses",
  "main": "index.js",
  "gypfile": true,
  "files": [
    "index.js",
    "src/",
    "binding.gyp",
    "scripts/",
    "prebuilds/",
    "iCUESDK.dll",
    "README.md"
  ],
  "scripts": {
    "install": "node scripts/setup-sdk.js --no-build",
    "build": "node-gyp rebuild",
    "clean": "node-gyp clean",
    "setup": "node scripts/setup-sdk.js",
    "prebuild": "prebuildify --napi --strip"
  },
  "keywords": [
    "corsair",
    "icue",
    "rgb",
    "lighting",
    "keyboard"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "node-gyp-build": "^4.8.4"
  },
  "devDependencies": {
    "node-addon-api": "^8.5.0",
    "node-gyp": "^10.3.1",
    "prebuildify": "^6.0.1"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
